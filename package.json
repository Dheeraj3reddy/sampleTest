{
  "name": "cdnexample",
  "version": "2.1.1",
  "description": "example project for CI/CD pipeline for static assets",
  "main": "index.js",
  "scripts": {
    "clean": "grunt clean-all",
    "build": "grunt build",
    "builddev": "grunt build-dev",
    "start": "webpack-dev-server",
    "watch": "webpack --progress --watch",
    "test": "jest",
    "coverage": "jest --collect-coverage",
    "postinstall": "echo 'nothing to do for postinstall'",
    "report-dependencies-tessa": "bash ./tessa-npm-plugin.sh update",
    "test-dependencies-tessa": "bash ./tessa-npm-plugin.sh check --errorOnNewVulnerability"
  },
  "repository": {
    "type": "git",
    "url": "git@git.corp.adobe.com:EchoSign/cdnexample.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^27.1.0",
    "babel-plugin-transform-require-context": "^0.1.1",
    "babel-polyfill": "~6.23.0",
    "bundle-loader": "~0.5.5",
    "eslint": "~3.19.0",
    "eslint-loader": "~1.7.1",
    "file-loader": "~1.1.5",
    "grunt": "~1.0.1",
    "grunt-contrib-clean": "~1.0.0",
    "grunt-contrib-copy": "~1.0.0",
    "grunt-env": "~0.4.4",
    "grunt-webpack": "~2.0.1",
    "html-loader-jest": "^0.2.1",
    "jest": "^27.2.1",
    "jest-cli": "^27.2.1",
    "load-grunt-tasks": "~3.5.2",
    "phantomjs-prebuilt": "~2.1.14",
    "webpack": "~2.2.1",
    "webpack-dev-server": "~2.4.2"
  },
  "dependencies": {
    "npmlibexample": "1.0.0",
    "es6-promise": "~4.1.0",
    "jquery": "~3.5.1",
    "underscore": "~1.12.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "js/**/*.{js,jsx}",
      "!**/nls/**",
      "!css/**/**",
      "!**/node_modules/**",
      "!test/**/**"
    ],
    "transform": {
      "^.+\\.html$": "html-loader-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/test/**/*-test.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|md)$": "<rootDir>/test/mocks/file-mock.js",
      "\\.(css|less)$": "<rootDir>/test/mocks/style-mock.js",
      "^bundle-loader?.*!\\./(.*)$": "<rootDir>/js/nls/$1"
    }
  }
}
