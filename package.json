{
  "name": "cdnexample",
  "version": "3.0.0",
  "description": "example project for CI/CD pipeline for static assets",
  "main": "index.js",
  "scripts": {
    "clean": "grunt clean-all",
    "build": "grunt build",
    "builddev": "grunt build-dev",
    "start": "webpack serve",
    "watch": "webpack --progress --watch",
    "test": "jest",
    "coverage": "jest --coverage",
    "test:sonar": "grunt sonar",
    "postinstall": "echo 'nothing to do for postinstall'",
    "report-dependencies-tessa": "bash ./tessa-npm-plugin.sh update",
    "test-dependencies-tessa": "bash ./tessa-npm-plugin.sh check --errorOnNewVulnerability"
  },
  "repository": {
    "type": "git",
    "url": "git@git.corp.adobe.com:EchoSign/cdnexample.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^27.1.0",
    "babel-plugin-transform-require-context": "^0.1.1",
    "babel-polyfill": "~6.23.0",
    "eslint": "^7.32.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.27.5",
    "file-loader": "^6.2.0",
    "grunt": "^1.6.1",
    "grunt-contrib-clean": "~1.0.0",
    "grunt-contrib-copy": "~1.0.0",
    "grunt-env": "~0.4.4",
    "grunt-webpack": "^5.0.0",
    "html-loader-jest": "^0.2.1",
    "jest": "^27.2.1",
    "jest-cli": "^27.2.1",
    "load-grunt-tasks": "~3.5.2",
    "phantomjs-prebuilt": "~2.1.14",
    "sonarqube-scanner": "2.5.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "es6-promise": "~4.2.6",
    "jquery": "~3.5.1",
    "npmlibexample": "2.0.0",
    "underscore": "~1.12.1"
  },
  "jest": {
    "coverageDirectory": "test_coverage",
    "collectCoverageFrom": [
      "js/**/*.js"
    ],
    "transform": {
      "^.+\\.html$": "html-loader-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/test/**/*-test.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|md)$": "<rootDir>/test/mocks/file-mock.js",
      "\\.(css|less)$": "<rootDir>/test/mocks/style-mock.js"
    }
  }
}
